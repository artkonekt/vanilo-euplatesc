{
    "name": "konekt/vanilo-euplatesc",
    "description": "Euplatesc Payment Gateway Support for Vanilo (Laravel)",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/artkonekt/vanilo-euplatesc"
    },
    "require": {
        "php": "^7.3",
        "konekt/concord": "^1.5",
        "vanilo/payment": "2.1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Konekt\\Euplatesc\\": "src"
        }
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "monolog/monolog": "^1.22",
        "orchestra/testbench": "~4.0|~5.0|~6.0",
        "phpunit/phpunit" : "8 - 9",
        "mockery/mockery": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\Euplatesc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
